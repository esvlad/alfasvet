{{ header }}
<div id="information" class="container information information{{id}}">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
    {% endfor %}
  </ul>
  <h2>{{ heading_title }}</h2>
  <div class="information_content">
    {{ description }}
  </div>
</div>

